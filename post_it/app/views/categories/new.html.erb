<%= render 'shared/content_title', title: 'New Category' %>
<%= render 'shared/errors', obj: @category %>

<div class = "well">
  <%= form_for(@category) do |f| %>
    <p>
      <%= f.label :name %>
      <%= f.text_field :name, autofocus: true %>
    </p>

    <p>
      <%= f.submit 'Create Category', class: 'btn btn-postit-forms' %> &nbsp &nbsp
      <%= link_to "Cancel", root_path %>
    </p>
  <% end %>



</div>


